{
    "name": "ghostzero/twitch-toolkit",
    "description": "This package offers a twitch toolkit to simplify certain processes using laravel framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "René Preuß",
            "email": "rene@preuss.io"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0",
        "romanzipp/laravel-twitch": "dev-next",
        "firebase/php-jwt": "^5.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "GhostZero\\TwitchToolkit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GhostZero\\TwitchToolkit\\Providers\\TwitchToolkitServiceProvider"
            ]
        }
    }
}
