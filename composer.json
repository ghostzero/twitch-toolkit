    "description": "This package offers a twitch toolkit to simplify certain processes using laravel framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "René Preuß",
            "email": "rene@preuss.io"
        }
    ],
    "require": {
        "ext-json": "*",
        "laravel/framework": "5.8.*|^7.0|^8.0",
        "orchestra/testbench": "^3.7|^5.0|^6.0",
        "phpunit/phpunit": "^7.5|^8.5|^9.0",
        "romanzipp/laravel-twitch": "^2.0",
        "firebase/php-jwt": "^5.2"
    },
    "autoload": {
